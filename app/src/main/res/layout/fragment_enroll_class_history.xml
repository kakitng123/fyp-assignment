<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:android="http://schemas.android.com/apk/res/android">

<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".frontend.EnrollClassHistoryFragment">

    <TextView
        android:id="@+id/tvHistoryEnrollTitle"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:text="Enroll History Fragment" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/historyEnrollRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:listitem="@layout/user_enroll_history_card" />
</FrameLayout>
</layout>